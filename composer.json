        "require": {
              "verbi/yii2-extended-activerecord": ">=1.2.0",
              "npm-asset/bootpag": ">=1.2.0",
              "bower-asset/jquery.lazyload": ">=1.9",
              "bower-asset/dropzone": ">=4.0.1",
              "kartik-v/yii2-helpers": ">=v1.3.6",
              "kartik-v/yii2-detail-view": ">=v1.7.5",
              "kartik-v/yii2-editable": ">=v1.7.4",
              "kartik-v/yii2-money": ">=v1.2.1",
              "kartik-v/yii2-slider": ">=v1.3.2",
              "kartik-v/yii2-widgets": "*",
              "akavov/yii2-bootstrap-tokenfield": ">=0.1.0",
              "drsdre/yii2-wizardwidget": ">=1.2.3",
              "unclead/yii2-multiple-input": ">=1.0",
              "bower-asset/scriptjs": "*"
