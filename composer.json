	"keywords": ["yii2", "extension", "helpers", "html"],
	"type": "yii2-extension",
	"license": "GPLv3",
	"authors": [
		{
			"name": "Philip Verbist",
			"email": "philip.verbist@gmail.com"
		}
	],
	"support": {
